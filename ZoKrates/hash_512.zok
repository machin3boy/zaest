import "utils/pack/u32/pack256" as pack256;
import "hashes/sha256/512bit" as hash512;
import "./field_to_u32_8" as field_to_u32_8;

def main(field inputValueA, field inputValueB) -> field {

    u32[8] inputValueArrayA = field_to_u32_8(inputValueA);
    u32[8] inputValueArrayB = field_to_u32_8(inputValueB);
    u32[8] computedHash     = hash512(inputValueArrayA, inputValueArrayB);

    return pack256(computedHash);
}

